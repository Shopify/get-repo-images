{
  "compileOnSave": false,
  "compilerOptions": {
    "outDir": "dist",
    "rootDir": "src",
    "baseUrl": "src",
    "composite": true,
    "declaration": true,
    "declarationMap": true,
    "pretty": true,
    "moduleResolution": "node",
    "target": "ESNext",
    "downlevelIteration": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "strictNullChecks": true,
    "noImplicitAny": false,
    "sourceMap": false,
    "noEmitOnError": false,
    "experimentalDecorators": true,
    "noUnusedParameters": true,
    "noUnusedLocals": true,
    "importHelpers": true,
    "module": "ESNext",
    "lib": [
      "ESNext"
    ]
  },
  "include": [
    "./src/**/*.ts"
  ],
}
